{anchors}`} />\r\n        <div className={`fullpage-content`}>\r\n          <h1>{text}</h1>\r\n          <div className={`fullpage-main-content`}>\r\n            {child && child}\r\n          </div>\r\n        </div> \r\n      </div>\r\n    ))\r\n  }\r\n\r\n  render() {\r\n    if (!fullpageList.length) {\r\n      return null\r\n    }\r\n\r\n    const NavComponent = () => (\r\n      <div\r\n        id={`myMenu`}\r\n        className={`homePage-nav`}>\r\n        <ul className='actions'>\r\n          {this.renderNavList(navList)}\r\n        </ul>\r\n      </div>\r\n    )\r\n\r\n    return (\r\n      <Fragment>\r\n        <Header>\r\n          <NavComponent />\r\n        </Header>\r\n        <div className={`main-page`}>\r\n          <ReactFullpage\r\n            navigation\r\n            loopTop={true}\r\n            menu={`#myMenu`}\r\n            loopBottom={true}\r\n            lazyLoading={true}\r\n            lockAnchors={false}\r\n            easingcss3={`ease`}\r\n            scrollingSpeed={1000}\r\n            loopHorizontal={true}\r\n            licenseKey={'HomePageKey'}\r\n            sectionSelector={SECTION_SEL}\r\n            pluginWrapper={pluginWrapper}\r\n            sectionsColor={this.state.sectionsColor}\r\n            anchors={navList.map(({ anchors }) => (anchors))}\r\n            onLeave={() => this.onLeave}\r\n\r\n            render={component=> (\r\n              <ReactFullpage.Wrapper>\r\n                {this.renderFullpageList(fullpageList)}\r\n              </ReactFullpage.Wrapper>\r\n            )}\r\n          />\r\n        </div>\r\n      </Fragment>  \r\n    )\r\n  }\r\n}\r\n\r\nexport default HomePageContainer\r\n","H:\\Webs\\slimhongweb\\client\\src\\api\\api.js",[],"H:\\Webs\\slimhongweb\\client\\src\\api\\cppsummary.js",[],"H:\\Webs\\slimhongweb\\client\\src\\Components\\HomePageComponents\\Header\\index.js",[],"H:\\Webs\\slimhongweb\\client\\src\\Components\\HomePageComponents\\Cpp\\MainList\\index.js",[],"H:\\Webs\\slimhongweb\\c